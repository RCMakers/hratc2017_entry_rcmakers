<launch>
   <param name="use_sim_time" value="true"/>
   <node name="locator" pkg="robot_pose_ekf" type="robot_pose_ekf" name="robot_pose_ekf" output="screen">
<!--        <param name="base_footprint_frame" value="base_link"/>-->
        <remap from="robot_pose_ekf/odom_combined" to="robot_pose_ekf/odom"/>
        <remap from="odom" to="p3at/odom"/>
        <remap from="imu_data" to="imu/data"/>
        <param name="output_frame" value="odom"/>
        <param name="freq" value="100.0"/>
        <param name="sensor_timeout" value="1.0"/>
        <param name="odom_used" value="true"/>
        <param name="imu_used" value="true"/>
        <param name="imu_absolute" value="true"/>
        <param name="vo_used" value="false"/>
        <param name="gps_used" value="false"/>
        <param name="debug" value="false"/>
        <param name="self_diagnose" value="false"/>
    </node>
   <node name="navigator" pkg="hratc2017_entry_rcmakers" type="navigator.py" respawn="true" output="screen"/>
   <node name="detector" pkg="hratc2017_entry_rcmakers" type="detector.py" respawn="true" output="screen"/>
</launch>
