<launch>
   <include file="$(find hratc2017_framework)/launch/run_simulation.launch"></include>
   <node name="navigator" pkg="hratc2017_entry_rcmakers" type="navigator.py" respawn="true"/>
   <node name="detector" pkg="hratc2017_entry_rcmakers" type="detector.py" respawn="true"/>
   <node name="gps_conv" pkg="gps_common" type="utm_odometry_node">
      <remap from="odom" to="vo"/>
      <remap from="fix" to="/gps/fix"/>
      <param name="rot_covariance" value="99999"/>
      <param name="frame_id" value="base_footprint"/>
   </node>
   <node name="locator" pkg="robot_pose_ekf" type="robot_pose_ekf" respawn="true">
      <remap from="/odom" to="/p3at/odom"/>
      <remap from="/imu_data" to="/imu/data"/>
      <param name="output_frame" value="odom"/>
      <param name="sensor_timeout" value="1.0"/>
      <param name="odom_used" value="true"/>
      <param name="imu_used" value="true"/>
      <param name="gps_used" value="false"/>
      <param name="vo_used" value="true"/>
      <param name="debug" value="false"/>
      <param name="self_diagnose" value="false"/>
      <param name="freq" value="30.0"/>
   </node>
</launch>
