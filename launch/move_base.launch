<launch>  
 <include file="$(find amcl)/examples/amcl_omni.launch" />

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find hratc2017_entry_rcmakers)/data/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find hratc2017_entry_rcmakers)/data/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find hratc2017_entry_rcmakers)/data/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find hratc2017_entry_rcmakers)/data/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find hratc2017_entry_rcmakers)/data/base_local_planner_params.yaml" command="load" />
  </node>
</launch>
